{
  "launcher": {
    "properties": {
      "foo": "bar"
    },
    "managers": [
      {
        "name": "osgi",
        "properties": {
          "storageDirectory": "./osgi/cache",
          "startLevel": "80"
        }
      },
      {
        "name": "spring-boot"
      },
      {
        "name": "microprofile",
        "properties": {
          "enabled": false
        }
      }
    ],
    "services": [
      {
        "name": "log",
        "location": "/path/to/log-service.jar",
        "properties": {
          "patternLayout": "%m %n"
        }
      }
    ]
  },
  "profiles": [
    {
      "name": "myprofile",
      "properties": {
        "foo": "bar"
      },
      "classloader": {
        "order": "PARENT_FIRST",
        "priorities": [ { "type": "RESOURCE", "pattern": "org/slf4j/LoggerFactory" }, {"type": "CLASS", "name": "com.foo.[*]"} ],
        "urls": [ "/path/to/jar/file.jar", "/path/to/folder" ]
      }
    }
  ],
  "applications": [
    {
      "url": "/path/to/app/spring-boot.jar",
      "profiles": [ "myprofile" ],
      "services": [
        {
          "name": "spring-boot",
          "properties": {
            "enableHttp": true,
            "enablePrometheus": true
          }
        }
      ]
    },
    {
      "url": "/path/to/osgi/bundle.jar",
      "type": "osgi"
    }
  ]
}