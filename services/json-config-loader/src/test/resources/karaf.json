{
  "properties": {
    "foo": "bar",
    "lifecycle.enabled": true,
    "log.patternLayout": "%m %n",
    "osgi.storageDirectory": "./osgi/cache",
    "osgi.priority": 1
  },
  "profiles": [
    {
      "name": "myprofile",
      "properties": {
        "foo": "bar"
      },
      "classloader": {
        "order": "PARENT_FIRST",
        "priorities": [ { "type": "RESOURCE", "pattern": "org/slf4j/LoggerFactory" }, {"type": "CLASS", "name": "com.foo.[*]"} ],
        "urls": [ "/path/to/jar/file.jar", "/path/to/folder" ]
      }
    }
  ],
  "applications": [
    {
      "url": "/path/to/app/spring-boot.jar",
      "profiles": [ "myprofile" ],
      "type": "spring-boot",
      "properties": {
        "enableHttp": true,
        "enablePrometheus": true
      }
    },
    {
      "url": "/path/to/osgi/bundle.jar",
      "type": "osgi"
    }
  ]
}