{
  "launcher": {
    "properties": {
      "foo": "bar"
    },
    "services": [
      {
        "name": "osgiModuleService",
        "properties": {
          "cacheDirectory": "${env:KARAF_DATA}/osgi/cache",
          "bootDelegation": "com.sun.*,javax.transaction",
          "systemPackages": "java.lang,java.util"
        }
      },
      {
        "name": "springBootModuleService"
      },
      {
        "name": "microprofileModuleService",
        "properties": {
          "enabled": false
        }
      }
    ]
  },
  "profiles": [
    {
      "name": "myprofile",
      "properties": {
        "foo": "bar"
      },
      "classloader": {
        "order": "PARENT_FIRST",
        "priorities": [ { "type": "RESOURCE", "pattern": "org/slf4j/LoggerFactory" }, {"type": "CLASS", "name": "com.foo.[*]"} ]
        "urls": [ "/path/to/jar/file.jar", "/path/to/folder" ]
      }
    }
  ],
  "applications": [
    {
      "url": "/path/to/app/spring-boot.jar",
      "profiles": [ "myprofile" ],
      "services": [
        {
          "name": "spring-boot",
          "properties": {
            "enableHttp": true,
            "enablePrometheus": true
          }
        }
      ]
    },
    {
      "url": "/path/to/osgi/bundle.jar",
      "type": "osgi"
    }
  ]
}